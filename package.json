  "bugs": {
    "url": "https://github.com/stephanel/angjest/issues"
  },
  "homepage": "https://github.com/stephanel/angjest#readme",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.13.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.1.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
